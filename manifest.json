{
  "manifest_version": 3,
  "name": "RSSBox",
  "version": "0.0.9",
  "description": "Read your RSS feeds with a simple and clean interface",
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "alarms",
    "offscreen"
  ],
  "host_permissions": [
    "https://*/",
    "http://*/"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "options_page": "options/options.html"
}